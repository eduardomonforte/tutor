    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.2.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.1.0",
    "express": "^4.16.3",
    "express-graphql": "^0.7.1",
    "express-session": "^1.15.6",
    "graphql": "^14.1.1",
    "http-errors": "~1.6.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "morgan": "~1.9.0",
